{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-login": "ng build --project=login",
    "dev": "concurrently --kill-others \"npm run mock-server\" \"ng serve --configuration development\"",
    "mock-server": "node mock-server/app.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers ChromeHeadless --watch=false",
    "test-login": "ng test --project=login --browsers ChromeHeadless --watch=false",
    "lint": "ng lint"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.8",
    "@angular/animations": "^20.3.4",
    "@angular/common": "^20.3.4",
    "@angular/compiler": "^20.3.4",
    "@angular/core": "^20.3.4",
    "@angular/forms": "^20.3.4",
    "@angular/material": "^20.2.8",
    "@angular/platform-browser": "^20.3.4",
    "@angular/router": "^20.3.4",
    "@fontsource/roboto": "^5.2.6",
    "material-icons": "^1.13.14",
    "ngx-cookie-service": "^20.1.0",
    "ngx-logger": "^5.0.12",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.5",
    "@angular/cli": "^20.3.5",
    "@angular/compiler-cli": "^20.3.4",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.4.0",
    "body-parse": "^0.1.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.33.0",
    "express": "^5.1.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.40.0"
  }
}
